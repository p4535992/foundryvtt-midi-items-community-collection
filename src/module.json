      "ko-fi": "",
      "reddit": "",
      "email": ""
    }
  ],
  "type": "module",
  "socket": true,
  "includes": [
    "./assets/**",
    "./lang/**",
    "./scripts/**",
    "./styles/**",
    "./templates/**",
    "./module.json",
    "./README.md",
    "./icons/**",
    "./packs/**"
