{
  "name": "midi-items-community",
  "title": "Midi Items Collection",
  "description": "DnD 5e items, features and spells configured for Dynamic Active Effects and Midi QoL",
  "version": "0.3.0",
  "author": "p4535992",
  "authors": [
    {
      "name": "p4535992",
      "url": "https://github.com/p4535992",
